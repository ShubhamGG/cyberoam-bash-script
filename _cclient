function _cyberoam_client()
{
	local word=${COMP_WORDS[COMP_CWORD]}
	local list=~/.cyberoam_*.conf
	local list1=""
	if [[ $COMP_CWORD == 1 ]]; then
		for i in $list
		do
			local a=${i%.*}
			list1="$list1 ${a#*_}"
		done
	fi
	COMPREPLY=($(compgen -W "${list1}" -- ${word}))
	return 0
}
complete -F _cyberoam_client cyberoam-client